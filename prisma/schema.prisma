generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Key {
  id            Int       @id @default(autoincrement())
  publicKeyE    Int  
  publicKeyN    Int  
  owner         String
  transcript    Transcript[]
}

model Transcript {
  nim           String     @id
  nama          String
  kodeMk1       String
  namaMk1       String
  nilai1        String
  sks1          String
  kodeMk2       String
  namaMk2       String
  nilai2        String
  sks2          String
  kodeMk3       String
  namaMk3       String
  nilai3        String
  sks3          String
  kodeMk4       String
  namaMk4       String
  nilai4        String
  sks4          String
  kodeMk5       String
  namaMk5       String
  nilai5        String
  sks5          String
  kodeMk6       String
  namaMk6       String
  nilai6        String
  sks6          String
  kodeMk7       String
  namaMk7       String
  nilai7        String
  sks7          String
  kodeMk8       String
  namaMk8       String
  nilai8        String
  sks8          String
  kodeMk9       String
  namaMk9       String
  nilai9        String
  sks9          String
  kodeMk10      String
  namaMk10      String
  nilai10       String
  sks10         String
  totalSks      String
  ipk           String
  key           Key       @relation(fields: [keyId], references: [id])
  keyId         Int            
  publicKeyE    String
  publicKeyN    String
  signature     String
  encryptKey    String
}